    "bcrypt": "^5.1.0",
    "next": "12.3.1",
    "next-auth": "^4.13.0",
    "nodemailer": "^6.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.55.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.7.20",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.12",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.5.0",
    "prisma-merge": "^0.2.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
